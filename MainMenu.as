package  {	import flash.display.Sprite;	import flash.text.TextField;	import flash.events.MouseEvent;	import flash.events.Event;		public class MainMenu extends Sprite {		public function MainMenu() {			setupMenu();		}				private function setupMenu():void {						//start game textfield			var start_tf:TextField = Static.textField('Start game ->', 36);			start_tf.y = 120;			addChild(start_tf);									//how to play textfield			var how_tf:TextField = Static.textField('How to play ->', 36);			how_tf.y = 200;			addChild(how_tf);						//scores textfield			var scores_tf:TextField = Static.textField('High scores ->', 36);			scores_tf.y = 280;			addChild(scores_tf);						start_tf.addEventListener(MouseEvent.CLICK, clickStart);			how_tf.addEventListener(MouseEvent.CLICK, clickHow);			scores_tf.addEventListener(MouseEvent.CLICK, clickScores);		}				private function clickScores(e:MouseEvent):void {			dispatchEvent(new Event('highScoresClicked'));		}				private function clickHow(e:MouseEvent):void {			dispatchEvent(new Event('howToClicked'));		}				private function clickStart(e:MouseEvent):void {			dispatchEvent(new Event('startGameClicked'));		}	}	}